sno=int(input("Enter Student Number"))
sname=input("Enter Student Name")
group=input("Enter Branch name")
s1=int(input("Enter Maths marks"))
s2=int(input("Enter Physics marks"))
s3=int(input("Enter Chemistry marks"))
Total=s1+s2+s3
avg=Total//3
if avg>=90:
    print("O-Grade")
elif avg>=80:
    print("A-Grade")
elif avg>=70:
    print("B-Grade")
elif avg>=60:
    print("C-Grade")
elif avg>=50:
    print("D-Grade")
else:
    print("Pass")
print(Total)
print(avg)
